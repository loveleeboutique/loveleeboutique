- if spree_current_user
  %li= link_to t(:my_account), spree.account_path
  %li#link-to-cart{"data-hook" => ""}= link_to_cart
  %li= link_to t(:logout), spree.destroy_user_session_path
- else
  %li#link-to-login= link_to "SIGN IN", spree.login_path
  %li#link-to-cart{"data-hook" => ""}= link_to_cart

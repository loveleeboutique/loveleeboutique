<% f.fields_for :image do |cat_image_form| %>

    <%= cat_image_form.field_container :attachment do %>
      <%= cat_image_form.label :attachment, 'Category Image' %>
      <%= cat_image_form.file_field :attachment %>
      <%= cat_image_form.error_message_on :attachment %>
    <% end %>

    <%= cat_image_form.field_container :alt_text do %>
      <%= cat_image_form.label :alt, 'Alt Text' %>
      <%= cat_image_form.text_field :alt %>
      <%= cat_image_form.error_message_on :alt %>
    <% end %>
<% end  %>
<%= f.submit 'Update' %>
